<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<stylesheets>
		<URL value="@DarkTheme.css" />
		<URL value="@Extensions.css" />
	</stylesheets>
	<children>
		<AnchorPane VBox.vgrow="NEVER" fx:id="commandBoxPlaceholder" styleClass="command-box-holder">
			<padding>
				<Insets top="5.0" bottom="5.0" left="5.0" right="5.0"/>
			</padding>
		</AnchorPane>
		<AnchorPane VBox.vgrow="NEVER" fx:id="resultDisplayPlaceholder" styleClass="content-holder" maxHeight="50">
			<padding>
				<Insets top="10.0" bottom="10.0" left="10.0" right="10.0"/>
			</padding>
		</AnchorPane>
		<AnchorPane VBox.vgrow="ALWAYS" id="taskListPanelPlaceholder" fx:id="taskListPanelPlaceholder" styleClass="content-holder">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="0.0"/>
			</padding>
		</AnchorPane>
		<AnchorPane fx:id="statusbarPlaceholder" minHeight="40" prefHeight="40" VBox.vgrow="NEVER" />
	</children>
</VBox>
