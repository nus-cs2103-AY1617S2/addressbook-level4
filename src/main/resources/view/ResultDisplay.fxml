<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="placeHolder" styleClass="anchor-pane-with-border" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
    </stylesheets>
    <AnchorPane fx:id="mainPane">
        <TextArea fx:id="resultDisplay" editable="false" style="-fx-background-color: derive (black, 30%);" styleClass="result-display" AnchorPane.bottomAnchor="0.0">
         <font>
            <Font name="LiSong Pro" size="14.0" />
         </font></TextArea>
    </AnchorPane>
</AnchorPane>
