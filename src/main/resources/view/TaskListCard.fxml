<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.image.*?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
            </columnConstraints>
            <children>
            <SplitPane dividerPositions="0.52, 0.85" prefHeight="80.0" prefWidth="200.0">
              <items>
                <AnchorPane minHeight="0.0" minWidth="260.0" prefHeight="80.0" prefWidth="260.0">
                     <children>
                            <VBox alignment="CENTER_LEFT" minHeight="60.0" prefHeight="65.0" prefWidth="260.0">
                                <stylesheets>
                                    <URL value="@DarkTheme.css" />
                                    <URL value="@Extensions.css" />
                                </stylesheets>
                                <padding>
                                    <Insets bottom="5" left="15" right="5" top="10" />
                                </padding>
                                <children>
                                      <HBox maxWidth="260.0" minWidth="260.0" prefHeight="25.0">
                                         <children>
                                             <Label fx:id="id" styleClass="cell_big_label" />
                                             <Label fx:id="name" styleClass="cell_big_label" text="\$first" />
                                         </children>
                                      </HBox>
                                    <Label fx:id="description" prefHeight="30.0" styleClass="cell_small_label" text="\$description" />
                                    <FlowPane fx:id="tags" maxHeight="20.0" prefWidth="260.0" />
                                </children>
                            </VBox>
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="time" Style="-fx-background-color:transparent" maxHeight="80.0" prefWidth="100.0">
                     <children>
                        <VBox prefHeight="80.0" prefWidth="100.0">
                         <padding>
                                    <Insets bottom="5" left="0" right="0" top="10" />
                                </padding>
                           <children>
                                    <Label fx:id="startTime" styleClass="cell_small_label" />
                                    <Label fx:id="to" styleClass="cell_small_label" text=" to " />
                                    <Label fx:id="deadline" styleClass="cell_small_label" />
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="20.0" prefHeight="20.0" prefWidth="20.0">
                     <children>
                              <Circle fx:id="labelBullet" radius="10" styleClass="taglist-item-bullet" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0" />
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
            </children>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </children>
</HBox>
