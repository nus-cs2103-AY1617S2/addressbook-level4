<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" maxHeight="110.0" maxWidth="500.0" minWidth="500.0" style="-fx-border-color: black; -fx-border-width: 4;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <FlowPane fx:id="overdueTag" HBox.hgrow="ALWAYS">
         <children>
            <TitledPane expanded="false" maxWidth="500.0" minWidth="490.0" style="-fx-border-color: transparent; -fx-background-color: transparent;" stylesheets="@DarkTheme.css" wrapText="true">
               <content>
                    <GridPane maxHeight="60.0" maxWidth="420.0" minHeight="60.0" minWidth="420.0">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
                        </columnConstraints>
                        <children>
                            <VBox alignment="CENTER_LEFT" maxWidth="420.0" minWidth="420.0" translateY="-10.0">
                                <stylesheets>
                                    <URL value="@DarkTheme.css" />
                                    <URL value="@Extensions.css" />
                                </stylesheets>
                                <padding>
                                    <Insets left="15.0" right="5.0" />
                                </padding>
                                <children>
                                    <Label fx:id="instruction" maxWidth="310.0" minWidth="310.0" styleClass="cell_small_label_instruction" text="\$instruction" translateX="-10.0" />
                                    <Label fx:id="priority" maxWidth="310.0" minWidth="310.0" styleClass="cell_small_label_priority" text="\$priority" translateX="-10.0" />
                                </children>
                            </VBox>
                        </children>
                     <rowConstraints>
                        <RowConstraints />
                     </rowConstraints>
                    </GridPane>
               </content>
               <graphic>
                  <VBox maxWidth="410.0" minWidth="410.0">
                     <children>
                        <VBox>
                           <children>
                              <HBox maxWidth="500.0">
                                 <children>
                                             <Label fx:id="id" minWidth="30.0" style="-fx-background-color: transparent; -fx-text-fill: black; -fx-font-family: Arial; -fx-font-size: 24; -fx-font-weight: bold;" styleClass="cell_big_label" HBox.hgrow="ALWAYS" />
                                             <Label fx:id="title" maxWidth="450.0" style="-fx-background-color: transparent; -fx-text-fill: black; -fx-font-size: 24; -fx-font-family: Arial; -fx-font-weight: bold;" styleClass="cell_big_label" text="\$first" wrapText="true" />
                                    <FlowPane fx:id="eventTag" maxWidth="10.0" prefWrapLength="30.0" styleClass="events" stylesheets="@DarkTheme.css" translateY="5.0" />
                                    <FlowPane fx:id="recurringTag" maxWidth="10.0" prefWrapLength="30.0" styleClass="events" translateX="3.0" translateY="5.0" />
                                 </children>
                              </HBox>
                                    <Label fx:id="date" minWidth="350.0" style="-fx-background-color: transparent; -fx-text-fill: black;" styleClass="cell_small_label_date" text="\$date" translateX="27.0" wrapText="true" />
                           </children>
                        </VBox>
                              <FlowPane fx:id="tags" maxWidth="410.0" minWidth="410.0" prefWrapLength="300.0" translateX="27.0" />
                     </children>
                  </VBox>
               </graphic>
            </TitledPane>
         </children>
      </FlowPane>
    </children>
</HBox>
