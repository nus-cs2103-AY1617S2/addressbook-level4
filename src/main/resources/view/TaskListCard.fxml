<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="40.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
               <Label fx:id="id" prefHeight="35.0" prefWidth="43.0" styleClass="cell_big_label" text="ID" textFill="#389ba4" HBox.hgrow="NEVER">
         <font>
            <Font name="Helvetica Light" size="22.0" />
         </font>
         <HBox.margin>
            <Insets left="20.0" top="-4.0" />
         </HBox.margin></Label>
      <VBox prefHeight="36.0" prefWidth="198.0" HBox.hgrow="ALWAYS">
         <children>
                     <Label fx:id="name" prefHeight="25.0" prefWidth="248.0" styleClass="cell_big_label" text="TaskTitle" textFill="#105b63">
               <font>
                  <Font name="Helvetica" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
            <Label fx:id="taskDate" prefHeight="11.0" prefWidth="251.0" text="taskDate" textFill="#6b6868">
               <font>
                  <Font name="Helvetica" size="9.0" />
               </font>
               <VBox.margin>
                  <Insets left="2.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
            <FlowPane fx:id="tags" prefWidth="200.0" />
    </children>
</HBox>
