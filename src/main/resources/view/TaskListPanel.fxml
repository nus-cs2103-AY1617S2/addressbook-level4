<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@Extensions.css">
    <children>
    	<JFXListView fx:id="uilist" minHeight="550.0" prefHeight="550.0">
			<JFXListView fx:id="todayTaskListView" prefHeight="1000.0" prefWidth="500.0">
				<groupnode>
					<Label>Today</Label>
				</groupnode>
			</JFXListView>
      		<JFXListView fx:id="futureTaskListView" prefHeight="1000.0" prefWidth="500.0">
				<groupnode>
					<Label>Future</Label>
				</groupnode>
			</JFXListView>
		</JFXListView>
    </children>
   <padding>
      <Insets top="15.0" />
   </padding>
</VBox>
