	<VBox prefWidth="550.0">
	      <children>
	      	<AnchorPane>
	          
	          <TitledPane animated="false" prefWidth="550.0" text="Today" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
